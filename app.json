{
  "expo": {
    "name": "Cedae",
    "slug": "Cedae",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/torneira.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cedaeSplash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "A(PRODUCT_NAME) Necessita usar sua localização"
          }
        ]
      ]
    }
  }
}

